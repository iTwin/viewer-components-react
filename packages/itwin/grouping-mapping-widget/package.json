  "scripts": {
    "build": "webpack && npm run copy-files ",
    "clean": "rimraf lib .rush/temp/package-deps*.json",
    "copy-files": "ncp README.md lib/README.md && ncp LICENSE.md lib/LICENSE.md",
    "cover": "",
    "lint": "npm run lint:ts",
    "lint:fix": "npm run lint:ts -- --fix */**/*.{js,ts,tsx,scss}",
