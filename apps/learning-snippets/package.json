{
  "name": "learning-snippets",
  "version": "0.0.0",
  "description": "Learning snippets for documentation",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "npm run link:deps && tsc",
    "build:watch": "tsc -w",
    "clean": "rimraf lib build",
    "cover": "npm run -s test",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "test": "cross-env NODE_OPTIONS=\"--import ../../node-hooks/ignore-styles/register.cjs\" mocha --enable-source-maps --config ./.mocharc.json",
    "audit": "pnpm audit --audit-level=high",
    "docs": "betools extract --fileExt=ts,tsx --extractFrom=./src --recursive --out=./build/docs/extract",
    "link:deps": "node ./scripts/linkWorkspaceDeps.cjs"
  },
  "dependencies": {
    "@itwin/appui-abstract": "^4.10.10",
    "@itwin/appui-react": "^5.3.0",
    "@itwin/build-tools": "^4.10.10",
    "@itwin/components-react": "^5.3.0",
    "@itwin/core-backend": "^4.10.10",
    "@itwin/core-bentley": "^4.10.10",
    "@itwin/core-common": "^4.10.10",
    "@itwin/core-frontend": "^4.10.10",
    "@itwin/core-geometry": "^4.10.10",
    "@itwin/core-orbitgt": "^4.10.10",
    "@itwin/core-quantity": "^4.10.10",
    "@itwin/core-react": "^5.3.0",
    "@itwin/core-telemetry": "^4.10.10",
    "@itwin/ecschema-metadata": "^4.10.10",
    "@itwin/ecschema-rpcinterface-common": "^4.10.10",
    "@itwin/ecschema-rpcinterface-impl": "^4.10.10",
    "@itwin/eslint-plugin": "^4.1.1",
    "@itwin/imodel-components-react": "^5.3.0",
    "@itwin/itwinui-react": "^3.17.3",
    "@itwin/presentation-backend": "^4.10.10",
    "@itwin/presentation-common": "^4.10.10",
    "@itwin/presentation-components": "^5.11.0",
    "@itwin/presentation-frontend": "^4.10.10",
    "@itwin/presentation-hierarchies": "^1.4.1",
    "@itwin/presentation-shared": "^1.2.0",
    "@itwin/presentation-testing": "^5.4.0",
    "@itwin/property-grid-react": "workspace:*",
    "@itwin/tree-widget-react": "workspace:*",
    "@itwin/unified-selection": "^1.3.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chai": "4.3.5",
    "@types/jsdom": "^21.1.7",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.19.80",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/sinon": "^17.0.4",
    "@types/sinon-chai": "^3.2.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "chai": "^4.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-unused-imports": "^3.2.0",
    "fast-xml-parser": "^4.5.3",
    "global-jsdom": "^9.2.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^23.2.0",
    "mocha": "^11.1.0",
