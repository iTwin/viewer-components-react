{
  "name": "@itwin/one-click-lca-react",
  "version": "0.7.1",
  "description": "An iTwin.js 3D Viewer Widget that interfaces with the iTwin Carbon Calculation.",
  "keywords": [
    "Bentley",
    "BIM",
    "iModel",
    "iTwin.js",
    "Carbon Calculation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/itwin/viewer-components-react/tree/master/packages/itwin/one-click-lca-widget"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/cjs/one-click-lca-widget.js",
  "module": "lib/esm/one-click-lca-widget.js",
  "typings": "lib/cjs/one-click-lca-widget",
  "scripts": {
    "build": "npm run -s dual-build && npm run -s copy:assets",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "clean": "rimraf lib",
    "copy:assets": "npm run -s copy:public && npm run -s copy:cjs && npm run -s copy:esm",
    "copy:cjs": "cpx \"./src/**/*.scss\" ./lib/cjs",
    "copy:esm": "cpx \"./src/**/*.scss\" ./lib/esm",
    "copy:public": "cpx \"./public/**/*\" ./lib/public",
    "dual-build": "npm run -s build:cjs && npm run -s build:esm",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" 1>&2",
    "lint:fix": "npm run lint -- --fix",
    "rebuild": "npm run clean && npm run build",
    "test": "vitest run"
  },
  "dependencies": {
    "@itwin/insights-client": "^0.16.0",
    "@itwin/itwinui-icons-react": "^2.2.0",
    "@itwin/itwinui-react": "^2.11.2"
  },
  "devDependencies": {
    "@itwin/appui-abstract": "^5.6.0",
    "@itwin/appui-react": "^5.6.0",
    "@itwin/build-tools": "^5.6.0",
    "@itwin/certa": "^3.6.1",
    "@itwin/components-react": "^5.6.0",
    "@itwin/core-bentley": "^5.6.0",
    "@itwin/core-common": "^5.6.0",
    "@itwin/core-frontend": "^5.6.0",
    "@itwin/core-geometry": "^5.6.0",
    "@itwin/core-markup": "^5.6.0",
    "@itwin/core-orbitgt": "^5.6.0",
    "@itwin/core-quantity": "^5.6.0",
    "@itwin/core-react": "^5.6.0",
    "@itwin/ecschema-metadata": "^5.6.0",
    "@itwin/eslint-plugin": "^6.0.0",
    "@itwin/imodel-components-react": "^5.6.0",
    "@itwin/presentation-common": "^5.6.0",
    "@itwin/presentation-frontend": "^5.6.0",
    "@itwin/webgl-compatibility": "^5.6.0",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^12.1.4",
    "@types/i18next": "^8.4.2",
    "@types/node": "^14.18.9",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-table": "^7.7.0",
    "@vitejs/plugin-react": "^4.3.0",
    "cpx2": "^3.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jsdom": "^25.0.0",
    "sass-embedded": "^1.86.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.2",
    "redux": "^4.1.0",
    "rimraf": "^3.0.2",
    "typescript": "~5.6.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@itwin/appui-abstract": "^5.0.0",
    "@itwin/appui-react": "^5.0.0",
    "@itwin/core-frontend": "^5.0.0",
    "@itwin/core-react": "^5.0.0",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  }
}
