{
  "name": "@bentley/markup-frontstage-react",
  "version": "2.0.2",
  "description": "Markup Frontstage React",
  "keywords": [
    "Bentley",
    "iModel Markup Frontstage React",
    "iModel.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/viewer-components-react/tree/master/packages/markup-frontstage"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/markup-frontstage-react.js",
  "typings": "lib/markup-frontstage-react",
  "scripts": {
    "build": " tsc 1>&2 && npm run copy:assets && npm run pseudolocalize",
    "clean": "rimraf lib .rush/temp/package-deps*.json",
    "copy:assets": "cpx \"./src/**/*.scss\" ./lib && cpx \"./public/**/*\" ./lib/public",
    "cover": "npm test",
    "lint": "",
    "pseudolocalize": "betools pseudolocalize --englishDir ./public/locales/en --out ./lib/public/locales/en-PSEUDO",
    "test": "jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "test",
      ".d.ts"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.(js|jsx|scss)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!@bentley/ui).+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "verbose": true
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "i18next": "^10.2.2",
    "i18next-browser-languagedetector": "^4.0.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@bentley/bentleyjs-core": "^2.16.0",
    "@bentley/build-tools": "^2.16.0",
    "@bentley/imodeljs-common": "^2.16.0",
    "@bentley/imodeljs-frontend": "^2.16.0",
    "@bentley/imodeljs-i18n": "^2.16.0",
    "@bentley/imodeljs-markup": "^2.16.0",
    "@bentley/presentation-frontend": "^2.16.0",
    "@bentley/ui-abstract": "^2.16.0",
    "@bentley/ui-components": "^2.16.0",
    "@bentley/ui-core": "^2.16.0",
    "@bentley/ui-framework": "^2.16.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^10.4.5",
    "@types/classnames": "^2.2.3",
    "@types/i18next": "^8.4.2",
    "@types/jest": "^24.0.15",
    "@types/react": "~16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/redux-mock-store": "^1.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-import-remove-resource-query": "^1.0.0",
    "concurrently": "^4.1.0",
    "cpx": "^1.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "jest-mock-console": "^1.0.0",
    "nodemon": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.1",
    "typescript": "^3.8.0"
  },
  "peerDependencies": {
    "@bentley/bentleyjs-core": "^2.16.0",
    "@bentley/build-tools": "^2.16.0",
    "@bentley/imodeljs-common": "^2.16.0",
    "@bentley/imodeljs-frontend": "^2.16.0",
    "@bentley/imodeljs-i18n": "^2.16.0",
    "@bentley/imodeljs-markup": "^2.16.0",
    "@bentley/presentation-frontend": "^2.16.0",
    "@bentley/ui-abstract": "^2.16.0",
    "@bentley/ui-components": "^2.16.0",
    "@bentley/ui-core": "^2.16.0",
    "@bentley/ui-framework": "^2.16.0",
    "@svgdotjs/svg.js": "^3.0.13",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  }
}
