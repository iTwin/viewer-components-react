{
  "name": "@itwin/grouping-mapping-widget",
  "version": "0.35.4",
  "description": "An iTwin.js 3D Viewer Widget that interfaces with the iTwin Reporting Platform.",
  "keywords": [
    "Bentley",
    "BIM",
    "iModel",
    "Reporting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/viewer-components-react.git",
    "directory": "packages/itwin/grouping-mapping-widget"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/cjs/grouping-mapping-widget.js",
  "module": "lib/esm/grouping-mapping-widget.js",
  "typings": "lib/cjs/grouping-mapping-widget",
  "scripts": {
    "build": "npm run -s dual-build && npm run -s copy:assets",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "clean": "rimraf lib",
    "copy:assets": "npm run -s copy:public && npm run -s copy:cjs && npm run -s copy:esm",
    "copy:cjs": "cpx \"./src/**/*.{scss,json}\" ./lib/cjs",
    "copy:esm": "cpx \"./src/**/*.{scss,json}\" ./lib/esm",
    "copy:public": "cpx \"./public/**/*\" ./lib/public",
    "cover": "npm test -- --coverage",
    "dual-build": "npm run -s build:cjs && npm run -s build:esm",
    "extract-api": "betools extract-api --entry=grouping-mapping-widget --apiReportFolder=./api --apiReportTempFolder=./api/temp --apiSummaryFolder=./api",
    "lint": "eslint -f visualstudio \"./src/**/*.{ts,tsx}\" 1>&2",
    "lint:fix": "npm run lint -- --fix",
    "pseudolocalize": "betools pseudolocalize --englishDir ./public/locales/en --out ./public/locales/en-PSEUDO",
    "rebuild": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": [
      "plugin:@itwin/ui",
      "prettier"
    ],
    "rules": {
      "no-duplicate-imports": "off",
      "@typescript-eslint/consistent-type-imports": "error",
      "@typescript-eslint/no-shadow": "off",
      "deprecation/deprecation": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/interactive-supports-focus": "off",
      "jsx-a11y/no-static-element-interactions": "off"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.5",
    "@dnd-kit/sortable": "7.0.1",
    "@dnd-kit/utilities": "3.2.0",
    "@itwin/imodels-access-common": "^4.1.2",
    "@itwin/imodels-access-frontend": "^4.1.2",
    "@itwin/imodels-client-management": "^4.2.1",
    "@itwin/insights-client": "^0.16.0",
    "@itwin/itwins-client": "^1.1.0",
    "@itwin/itwinui-icons-color-react": "2.1.0",
    "@itwin/itwinui-icons-react": "^2.2.0",
    "@itwin/itwinui-react": "^2.11.2",
    "@tanstack/query-core": "^4.36.1",
    "@tanstack/react-query": "^4.36.1",
    "classnames": "^2.3.1",
    "fast-equals": "^4.0.2",
    "immer": "^9.0.7",
    "lodash": "^4.17.21",
    "react-split": "2.0.14",
    "simple-react-validator": "^1.6.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@itwin/appui-abstract": "^5.6.0",
    "@itwin/appui-react": "^5.6.0",
    "@itwin/build-tools": "^5.6.0",
    "@itwin/components-react": "^5.6.0",
    "@itwin/core-bentley": "^5.6.0",
    "@itwin/core-common": "^5.6.0",
    "@itwin/core-frontend": "^5.6.0",
    "@itwin/core-geometry": "^5.6.0",
    "@itwin/core-i18n": "^5.6.0",
    "@itwin/core-markup": "^5.6.0",
    "@itwin/core-orbitgt": "^5.6.0",
    "@itwin/core-quantity": "^5.6.0",
    "@itwin/core-react": "^5.6.0",
    "@itwin/ecschema-metadata": "^5.6.0",
    "@itwin/eslint-plugin": "^3.7.7",
    "@itwin/imodel-components-react": "^5.6.0",
    "@itwin/presentation-common": "^5.6.0",
    "@itwin/presentation-components": "^5.6.0",
    "@itwin/presentation-frontend": "^5.6.0",
    "@itwin/webgl-compatibility": "^5.6.0",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.1.1",
    "@types/chai": "4.2.11",
    "@types/i18next": "^8.4.2",
    "@types/lodash": "^4.14.202",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.0.1",
    "@types/react-table": "^7.7.0",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@vitejs/plugin-react": "^4.3.0",
    "chai": "^4.2.0",
    "cpx2": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jsdom": "^25.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.2",
    "react-table": "^7.7.0",
    "redux": "^4.1.0",
    "rimraf": "^3.0.2",
    "sass-embedded": "^1.97.3",
    "sinon": "^17.0.1",
    "typemoq": "^2.1.0",
    "typescript": "~5.6.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@itwin/appui-abstract": "^5.6.0",
    "@itwin/appui-react": "^5.6.0",
    "@itwin/components-react": "^5.6.0",
    "@itwin/core-frontend": "^5.6.0",
    "@itwin/core-react": "^5.6.0",
    "@itwin/presentation-components": "^5.6.0",
    "@itwin/presentation-frontend": "^5.6.0",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  }
}
