    "react-dev-utils": "^11.0.4",
    "resolve-url-loader": "^4.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^6.0.11",
    "svgo-loader": "^3.0.0",
    "terser-webpack-plugin": "^4.2.3",
