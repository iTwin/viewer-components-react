    "@itwin/core-markup": "^4.9.1",
    "@itwin/core-orbitgt": "4.5.1",
    "@itwin/core-quantity": "^4.9.1",
    "@itwin/core-react": "^4.17.0",
    "@itwin/core-telemetry": "^4.9.1",
    "@itwin/ecschema-metadata": "^4.9.1",
    "@itwin/ecschema-rpcinterface-common": "^4.9.1",
    "@itwin/ecschema-rpcinterface-impl": "^4.9.1",
