{
  "name": "@itwin/grouping-mapping-widget",
  "version": "0.0.12",
  "description": "An iTwin.js 3D Viewer Widget that interfaces with the Bentley Reporting Platform.",
  "keywords": [
    "Bentley",
    "BIM",
    "iModel",
    "Reporting"
  ],
  "repository": {
