// Place your settings in this file to overwrite default and user settings.
{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/.nyc_output": true,
    "**/common/temp": true,
    "**/.rush": true
  },
  "files.exclude": {
    "**/.nyc_output": true,
    "**/.rush": true,
    "**/*.build.log": true,
    "**/*.build.error.log": true,
    "**/node_modules": true,
    "**/out": true,
    "**/lib": true,
    "common/temp": true,
  },
  "files.trimTrailingWhitespace": true,
  "files.associations": {
    "**/package.json": "json",
    "**/*.json": "jsonc"
  }
}