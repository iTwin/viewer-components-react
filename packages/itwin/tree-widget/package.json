    "chai-as-promised": "^7.1.1",
    "chai-jest-snapshot": "^2.0.0",
    "chai-subset": "1.6.0",
    "cpx2": "^3.0.0",
    "cross-env": "^7.0.3",
    "deep-equal": "^1.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-unused-imports": "^3.2.0",
    "fast-xml-parser": "^4.3.6",
    "global-jsdom": "^9.2.0",
