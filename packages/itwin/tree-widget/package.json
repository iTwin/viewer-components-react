{
  "name": "@itwin/tree-widget-react",
  "version": "0.10.0-dev.1",
  "description": "Tree Widget React",
  "keywords": [
    "Bentley",
    "iModel Tree Widget",
    "iTwin.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/itwin/viewer-components-react/tree/master/packages/itwin/tree-widget"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/cjs/tree-widget-react.js",
  "module": "lib/esm/tree-widget-react.js",
  "typings": "lib/cjs/tree-widget-react",
  "scripts": {
    "build": "npm run -s dual-build && npm run -s copy:assets",
    "dual-build": "npm run -s build:cjs && npm run -s build:esm",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "copy:assets": "npm run -s copy:public && npm run -s copy:cjs && npm run -s copy:esm",
    "copy:public": "cpx \"./public/**/*\" ./lib/public",
    "copy:cjs": "cpx \"./src/**/*.{scss,json}\" ./lib/cjs",
    "copy:esm": "cpx \"./src/**/*.{scss,json}\" ./lib/esm",
    "cover": "npm run test",
    "lint": "eslint -f visualstudio \"./src/**/*.{ts,tsx}\" 1>&2",
    "lint:fix": "npm run lint -- --fix",
    "pseudolocalize": "betools pseudolocalize --englishDir ./public/locales/en --out ./lib/public/locales/en-PSEUDO",
    "test": "mocha \"./lib/cjs/test/**/*.test.js\"",
    "clean": "rimraf lib .rush/temp/package-deps*.json",
    "rebuild": "npm run clean && npm run build"
  },
  "dependencies": {
    "@itwin/itwinui-icons-react": "^2.1.0",
    "@itwin/itwinui-react": "^2.7.0",
    "@itwin/itwinui-variables": "^2.0.0",
    "classnames": "^2.3.1",
    "i18next": "^10.2.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.3",
    "rxjs": "^6.6.2"
  },
  "devDependencies": {
    "@itwin/appui-abstract": "^4.0.0-dev.104",
    "@itwin/appui-layout-react": "^4.0.0",
    "@itwin/appui-react": "^4.0.0",
    "@itwin/build-tools": "^4.0.0-dev.104",
    "@itwin/components-react": "^4.0.0",
    "@itwin/core-backend": "^4.0.0-dev.104",
    "@itwin/core-bentley": "^4.0.0-dev.104",
    "@itwin/core-common": "^4.0.0-dev.104",
    "@itwin/core-frontend": "^4.0.0-dev.104",
    "@itwin/core-geometry": "^4.0.0-dev.104",
    "@itwin/core-i18n": "^4.0.0-dev.104",
    "@itwin/core-markup": "^4.0.0-dev.104",
    "@itwin/core-orbitgt": "^4.0.0-dev.104",
    "@itwin/core-quantity": "^4.0.0-dev.104",
    "@itwin/core-react": "^4.0.0",
    "@itwin/core-telemetry": "^4.0.0-dev.104",
    "@itwin/ecschema-metadata": "^4.0.0-dev.104",
    "@itwin/eslint-plugin": "^3.7.0",
    "@itwin/imodel-components-react": "^4.0.0",
    "@itwin/presentation-backend": "^4.0.0-dev.104",
    "@itwin/presentation-common": "^4.0.0-dev.104",
    "@itwin/presentation-components": "^4.0.0",
    "@itwin/presentation-frontend": "^4.0.0-dev.104",
    "@itwin/presentation-testing": "^4.0.0",
    "@itwin/webgl-compatibility": "^4.0.0-dev.104",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/chai": "4.3.1",
    "@types/chai-jest-snapshot": "^1.3.4",
    "@types/deep-equal": "^1.0.1",
    "@types/i18next": "^8.4.2",
    "@types/jsdom-global": "3.0.2",
    "@types/mocha": "^8.2.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.0.1",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "^4.2.0",
    "chai-jest-snapshot": "^2.0.0",
    "cpx2": "^3.0.0",
    "deep-equal": "^1.0.0",
    "eslint": "^7.11.0",
    "jsdom": "^19.0.0",
    "jsdom-global": "3.0.2",
    "mocha": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.2.0",
    "typemoq": "^2.1.0",
    "typescript": "~4.4.0",
    "xmlhttprequest": "^1.8.0"
  },
  "peerDependencies": {
    "@itwin/appui-abstract": "^4.0.0-dev.104",
    "@itwin/appui-react": "^4.0.0",
    "@itwin/components-react": "^4.0.0",
    "@itwin/core-frontend": "^4.0.0-dev.104",
    "@itwin/core-react": "^4.0.0",
    "@itwin/presentation-components": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": [
      "plugin:@itwin/ui"
    ],
    "rules": {
      "@itwin/no-internal": [
        "error"
      ],
      "@typescript-eslint/no-floating-promises": [
        "error",
        {
          "ignoreIIFE": true
        }
      ]
    }
  }
}
