{
  "name": "@itwin/breakdown-trees-react",
  "version": "0.3.3",
  "description": "Package for breakdown trees",
  "keywords": [
    "Bentley",
    "iModel breakdown Tree",
    "iModel.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/viewer-components-react/tree/master/packages/itwin/breakdown-trees"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/cjs/breakdown-trees-react.js",
  "module": "lib/esm/breakdown-trees-react.js",
  "typings": "lib/cjs/breakdown-trees-react",
  "scripts": {
    "build": "npm run copy:public && npm run -s build:cjs && npm run -s build:esm",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs && npm run -s copy:cjs",
    "build:esm": "tsc 1>&2 --outDir lib/esm --module ES2020 && npm run -s copy:esm",
    "copy:cjs": "cpx \"./src/**/*.*css\" ./lib/cjs",
    "copy:esm": "cpx \"./src/**/*.*css\" ./lib/esm",
    "rebuild": "npm run clean && npm run build",
    "makegraph": "depcruise --exclude \"(node_modules|src/test)\" --output-type dot src > graphs/dependencygraph.svg",
    "copy:public": "cpx \"./public/**/*\" ./lib/public",
    "build:watch": "tsc -w",
    "clean": "rimraf lib",
    "cover": "npm test",
    "test": "mocha \"./lib/cjs/test/**/*.test.js\"",
    "test:watch": "npm test -- --reporter min --watch-extensions ts,tsx --watch",
    "lint": "eslint -f visualstudio \"./src/**/*.{ts,tsx}\" 1>&2",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "i18next": "^10.2.2"
  },
  "devDependencies": {
    "@itwin/appui-abstract": "^3.0.0",
    "@itwin/appui-layout-react": "^3.0.0",
    "@itwin/appui-react": "^3.0.0",
    "@itwin/build-tools": "^3.0.0",
    "@itwin/components-react": "^3.0.0",
    "@itwin/core-bentley": "^3.0.0",
    "@itwin/core-common": "^3.0.0",
    "@itwin/core-frontend": "^3.0.0",
    "@itwin/core-geometry": "^3.0.0",
    "@itwin/core-i18n": "^3.0.0",
    "@itwin/core-markup": "^3.0.0",
    "@itwin/core-orbitgt": "^3.0.0",
    "@itwin/core-quantity": "^3.0.0",
    "@itwin/core-react": "^3.0.0",
    "@itwin/eslint-plugin": "^3.0.0",
    "@itwin/imodel-components-react": "^3.0.0",
    "@itwin/measure-tools-react": "workspace:*",
    "@itwin/presentation-common": "^3.0.0",
    "@itwin/presentation-components": "^3.0.0",
    "@itwin/presentation-frontend": "^3.0.0",
    "@itwin/webgl-compatibility": "^3.0.0",
    "@testing-library/react": "^11.2.6",
    "@types/chai": "4.2.11",
    "@types/chai-jest-snapshot": "^1.3.4",
    "@types/enzyme": "3.10.5",
    "@types/faker": "^4.1.0",
    "@types/mocha": "^8.2.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.9",
    "@types/sinon": "^7.5.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-jest-snapshot": "^2.0.0",
    "chai-spies": "1.0.0",
    "cpx2": "^3.0.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "3.4.4",
    "eslint": "^7.11.0",
    "faker": "^4.1.0",
    "jsdom": "^13.2.0",
    "jsdom-global": "3.0.2",
    "mocha": "^8.3.2",
    "mock-local-storage": "^1.1.8",
    "nyc": "^15.1.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "7.2.6",
    "redux": "^4.0.4",
    "rimraf": "^3.0.2",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.2.0",
    "typemoq": "^2.1.0",
    "typescript": "^4.4.0",
    "xmlhttprequest": "^1.8.0"
  },
  "peerDependencies": {
    "@itwin/appui-abstract": "^3.0.0",
    "@itwin/appui-layout-react": "^3.0.0",
    "@itwin/appui-react": "^3.0.0",
    "@itwin/components-react": "^3.0.0",
    "@itwin/core-bentley": "^3.0.0",
    "@itwin/core-common": "^3.0.0",
    "@itwin/core-frontend": "^3.0.0",
    "@itwin/core-geometry": "^3.0.0",
    "@itwin/core-react": "^3.0.0",
    "@itwin/measure-tools-react": "workspace:^0.7.0",
    "@itwin/presentation-common": "^3.0.0",
    "@itwin/presentation-components": "^3.0.0",
    "@itwin/presentation-frontend": "^3.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "7.2.6"
  },
  "nyc": {
    "extends": "./node_modules/@itwin/build-tools/.nycrc",
    "require": [
      "ignore-styles",
      "jsdom-global/register",
      "source-map-support/register",
      "ts-node/register"
    ],
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": "plugin:@itwin/ui",
    "rules": {
      "@typescript-eslint/unbound-method": "off",
      "no-duplicate-imports": "off",
      "@typescript-eslint/consistent-type-imports": "error"
    }
  }
}
