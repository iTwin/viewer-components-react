{
  "name": "@itwin/grouping-mapping-widget",
  "version": "0.35.1",
  "description": "An iTwin.js 3D Viewer Widget that interfaces with the iTwin Reporting Platform.",
  "keywords": [
    "Bentley",
    "BIM",
    "iModel",
    "Reporting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/viewer-components-react.git",
    "directory": "packages/itwin/grouping-mapping-widget"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/cjs/grouping-mapping-widget.js",
  "module": "lib/esm/grouping-mapping-widget.js",
  "typings": "lib/cjs/grouping-mapping-widget",
  "scripts": {
    "build": "npm run -s dual-build && npm run -s copy:assets",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "clean": "rimraf lib",
    "copy:assets": "npm run -s copy:public && npm run -s copy:cjs && npm run -s copy:esm",
    "copy:cjs": "cpx \"./src/**/*.scss\" ./lib/cjs",
    "copy:esm": "cpx \"./src/**/*.scss\" ./lib/esm",
    "copy:public": "cpx \"./public/**/*\" ./lib/public",
    "cover": "npm test -- --coverage",
    "dual-build": "npm run -s build:cjs && npm run -s build:esm",
    "extract-api": "betools extract-api --entry=grouping-mapping-widget --apiReportFolder=./api --apiReportTempFolder=./api/temp --apiSummaryFolder=./api",
    "lint": "eslint -f visualstudio \"./src/**/*.{ts,tsx}\" 1>&2",
    "audit": "pnpm audit --audit-level=high",
    "lint:fix": "npm run lint -- --fix",
    "pseudolocalize": "betools pseudolocalize --englishDir ./public/locales/en --out ./public/locales/en-PSEUDO",
    "rebuild": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch "
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": [
      "plugin:@itwin/ui",
      "prettier"
    ],
    "rules": {
      "no-duplicate-imports": "off",
      "@typescript-eslint/consistent-type-imports": "error",
      "@typescript-eslint/no-shadow": "off",
      "deprecation/deprecation": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/interactive-supports-focus": "off",
      "jsx-a11y/no-static-element-interactions": "off"
    }
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "7.0.1",
    "@dnd-kit/utilities": "3.2.0",
    "@itwin/imodels-access-frontend": "^5.2.3",
    "@itwin/imodels-client-management": "^5.9.0",
    "@itwin/insights-client": "^0.15.1",
    "@itwin/itwins-client": "^1.6.1",
    "@itwin/itwinui-icons-color-react": "2.1.0",
    "@itwin/itwinui-icons-react": "^2.10.0",
    "@itwin/itwinui-react": "^2.12.26",
    "@tanstack/query-core": "^4.36.1",
    "@tanstack/react-query": "^4.36.1",
    "classnames": "^2.5.1",
    "fast-equals": "^4.0.3",
    "immer": "^9.0.21",
    "lodash": "^4.17.21",
    "react-split": "2.0.14",
    "simple-react-validator": "^1.6.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@itwin/appui-abstract": "^4.10.10",
    "@itwin/appui-layout-react": "^4.8.3",
    "@itwin/appui-react": "^4.17.6",
    "@itwin/build-tools": "^4.10.10",
    "@itwin/components-react": "^4.17.6",
    "@itwin/core-bentley": "^4.10.10",
    "@itwin/core-common": "^4.10.10",
    "@itwin/core-frontend": "^4.10.10",
    "@itwin/core-geometry": "^4.10.10",
    "@itwin/core-i18n": "^4.10.10",
    "@itwin/core-markup": "^4.10.10",
    "@itwin/core-orbitgt": "^4.10.10",
    "@itwin/core-quantity": "^4.10.10",
    "@itwin/core-react": "^4.17.6",
    "@itwin/core-telemetry": "^4.10.10",
    "@itwin/ecschema-metadata": "^4.10.10",
    "@itwin/eslint-plugin": "^3.8.0",
    "@itwin/imodel-components-react": "^4.17.6",
    "@itwin/presentation-common": "^4.10.10",
    "@itwin/presentation-components": "^4.4.1",
    "@itwin/presentation-frontend": "^4.10.10",
    "@itwin/webgl-compatibility": "^4.10.10",
    "@testing-library/dom": "^8.20.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.6.1",
    "@types/chai": "4.2.11",
    "@types/i18next": "^8.4.6",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.17.16",
    "@types/node": "^18.19.80",
    "@types/react": "^17.0.83",
    "@types/react-dom": "^17.0.26",
    "@types/react-redux": "^7.1.34",
    "@types/react-table": "^7.7.20",
    "@types/sinon": "^17.0.4",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.5.0",
    "cpx2": "^3.0.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.9",
    "react-table": "^7.8.0",
    "redux": "^4.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^17.0.1",
    "ts-jest": "^27.1.5",
    "typemoq": "^2.1.0",
    "typescript": "~5.0.4"
  },
  "peerDependencies": {
    "@itwin/appui-abstract": "^4.1.0",
    "@itwin/appui-react": "^4.1.0",
    "@itwin/components-react": "^4.1.0",
    "@itwin/core-frontend": "^4.1.0",
    "@itwin/core-react": "^4.1.0",
    "@itwin/presentation-components": "^4.1.0 || ^5.0.0",
    "@itwin/presentation-frontend": "^4.1.0",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  }
}
