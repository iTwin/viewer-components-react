{
  "include": ["src/**/*", "lib/cjs/**/*"],
  "exclude": ["src/test/**", "lib/cjs/test/**"],
  "extension": [".ts", ".tsx"],
  "reporter": ["text-summary", "lcov", "cobertura"],
  "temp-dir": "./lib/test/coverage/.nyc_output",
  "report-dir": "./lib/test/coverage",
  "check-coverage": true,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "lines": 100
}
