    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/chai": "4.3.5",
    "@types/chai-as-promised": "^7.1.1",
    "@types/chai-jest-snapshot": "^1.3.4",
    "@types/chai-subset": "1.3.3",
    "@types/deep-equal": "^1.0.1",
    "@types/jsdom": "^21.1.6",
    "@types/mocha": "^10.0.6",
