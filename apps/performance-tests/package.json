{
  "name": "vcr-performance-tests",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "benchmark:tree-widget": "cross-env NODE_OPTIONS=\"--import ../../node-hooks/ignore-styles/register.cjs\" npm run test:tree-widget -- -O BENCHMARK_OUTPUT_PATH=./tree-widget-benchmark.json",
    "build": "npm run link:deps && tsc",
    "clean": "rimraf lib temp",
    "docs": "betools extract --fileExt=ts --extractFrom=./src --recursive --out=./build/docs/extract",
    "lint": "eslint \"./src/**/*.ts\"",
    "test": "cross-env NODE_OPTIONS=\"--import ../../node-hooks/ignore-styles/register.cjs\" mocha --enable-source-maps --config ./.mocharc.json ./lib/**/*.test.js",
    "test:tree-widget": "cross-env NODE_OPTIONS=\"--import ../../node-hooks/ignore-styles/register.cjs\" mocha --enable-source-maps --config ./.mocharc.json ./lib/tree-widget/*.test.js",
    "test:recreate": "cross-env RECREATE=true npm run test",
    "link:deps": "node ./scripts/linkWorkspaceDeps.cjs"
  },
  "devDependencies": {
    "@itwin/appui-react": "^5.24.1",
    "@itwin/build-tools": "^5.7.0-dev.14",
    "@itwin/core-backend": "^5.7.0-dev.14",
    "@itwin/core-bentley": "^5.7.0-dev.14",
    "@itwin/core-common": "^5.7.0-dev.14",
    "@itwin/core-frontend": "^5.7.0-dev.14",
    "@itwin/core-geometry": "^5.7.0-dev.14",
    "@itwin/core-quantity": "^5.7.0-dev.14",
    "@itwin/ecschema-metadata": "^5.7.0-dev.14",
    "@itwin/ecschema-rpcinterface-common": "^5.7.0-dev.14",
    "@itwin/ecschema-rpcinterface-impl": "^5.7.0-dev.14",
    "@itwin/eslint-plugin": "^6.0.0",
    "@itwin/presentation-common": "^5.7.0-dev.14",
    "@itwin/presentation-core-interop": "^1.3.8",
    "@itwin/presentation-hierarchies": "^1.7.9",
    "@itwin/presentation-shared": "^1.2.8",
    "@itwin/presentation-testing": "^5.4.14",
    "@itwin/tree-widget-react": "workspace:*",
    "@itwin/unified-selection": "^1.6.6",
    "@types/chai": "^5.2.3",
    "@types/jsdom": "^27.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.8",
    "as-table": "^1.0.55",
    "chai": "^6.2.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "global-jsdom": "^28.0.0",
    "jsdom": "^28.0.0",
    "mocha": "^11.7.5",
    "rimraf": "^6.1.2",
    "rxjs": "^7.8.2",
    "test-utilities": "workspace:*",
    "typescript": "~5.9.3"
  }
}
