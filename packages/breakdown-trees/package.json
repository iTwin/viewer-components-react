  "scripts": {
    "build": "npm run copy:assets && tsc --sourceMap 1>&2",
    "build-prod": "tsc 1>&2 && npm run copy:assets",
    "rebuild": "npm run clean && npm run build",
    "makegraph": "depcruise --exclude \"(node_modules|src/test)\" --output-type dot src > graphs/dependencygraph.svg",
    "copy:assets": "cpx \"./src/assets/**/*\" ./lib/assets && cpx \"./src/**/*.scss\" ./lib && cpx \"./public/**/*\" ./lib/public",
    "build:watch": "tsc -w",
    "clean": "rimraf lib",
    "cover": "npm test",
    "test": "mocha --opts mocha.opts \"./lib/test/**/*.test.js\"",
    "test:watch": "npm test -- --reporter min --watch-extensions ts,tsx --watch",
    "lint": "tslint --project . 1>&2",
    "lintfix": "tslint --fix --project . 1>&2"
  },
