{
  "name": "@itwin/reports-config-widget-react",
  "version": "0.8.2",
  "description": "An iTwin.js 3D Viewer Widget that interfaces with the iTwin Reporting Platform.",
  "keywords": [
    "Bentley",
    "BIM",
    "iModel",
    "iTwin",
    "Reporting",
    "Reports"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/viewer-components-react/tree/master/packages/itwin/reports-config-widget"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/cjs/reports-config-widget-react.js",
  "module": "lib/esm/reports-config-widget-react.js",
  "typings": "lib/cjs/reports-config-widget-react",
  "scripts": {
    "build": "npm run -s dual-build && npm run -s copy:assets",
    "build:cjs": "tsc 1>&2 --outDir lib/cjs",
    "build:esm": "tsc 1>&2 --module ES2020 --outDir lib/esm",
    "clean": "rimraf lib",
    "copy:assets": "npm run -s copy:public && npm run -s copy:cjs && npm run -s copy:esm",
    "copy:cjs": "cpx \"./src/**/*.scss\" ./lib/cjs",
    "copy:esm": "cpx \"./src/**/*.scss\" ./lib/esm",
    "copy:public": "cpx \"./public/**/*\" ./lib/public",
    "cover": "npm test -- --coverage",
    "dual-build": "npm run -s build:cjs && npm run -s build:esm",
    "extract-api": "betools extract-api --entry=reports-config-widget-react --apiReportFolder=./api --apiReportTempFolder=./api/temp --apiSummaryFolder=./api",
    "lint": "eslint \"./src/**/*.{ts,tsx}\" 1>&2",
    "lint:fix": "npm run lint -- --fix",
    "pseudolocalize": "betools pseudolocalize --englishDir ./public/locales/en --out ./public/locales/en-PSEUDO",
    "rebuild": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@itwin/imodels-access-common": "^4.1.4",
    "@itwin/imodels-access-frontend": "^4.1.4",
    "@itwin/imodels-client-management": "^4.2.3",
    "@itwin/insights-client": "^0.16.0",
    "@itwin/itwinui-icons-color-react": "^2.1.0",
    "@itwin/itwinui-icons-react": "^2.4.0",
    "@itwin/itwinui-react": "^2.11.0",
    "classnames": "^2.3.1",
    "simple-react-validator": "^1.6.1"
  },
  "devDependencies": {
    "@faker-js/faker": "6.2.0",
    "@itwin/appui-abstract": "^5.6.0",
    "@itwin/appui-react": "^5.6.0",
    "@itwin/build-tools": "^5.6.0",
    "@itwin/components-react": "^5.6.0",
    "@itwin/core-bentley": "^5.6.0",
    "@itwin/core-common": "^5.6.0",
    "@itwin/core-frontend": "^5.6.0",
    "@itwin/core-geometry": "^5.6.0",
    "@itwin/core-markup": "^5.6.0",
    "@itwin/core-orbitgt": "^5.6.0",
    "@itwin/core-quantity": "^5.6.0",
    "@itwin/core-react": "^5.6.0",
    "@itwin/ecschema-metadata": "^5.6.0",
    "@itwin/eslint-plugin": "^6.0.0",
    "@itwin/imodel-components-react": "^5.6.0",
    "@itwin/presentation-common": "^5.6.0",
    "@itwin/presentation-frontend": "^5.6.0",
    "@itwin/webgl-compatibility": "^5.6.0",
    "@testing-library/dom": "^8.12.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^14.1.1",
    "@types/chai": "4.2.11",
    "@types/i18next": "^8.4.2",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-table": "^7.7.0",
    "chai": "^4.2.0",
    "cpx2": "^3.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "jsdom": "^25.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.2",
    "redux": "^4.1.0",
    "rimraf": "^3.0.2",
    "sass-embedded": "^1.86.0",
    "typemoq": "^2.1.0",
    "typescript": "~5.6.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "@itwin/appui-abstract": "^5.0.0",
    "@itwin/appui-react": "^5.0.0",
    "@itwin/core-bentley": "^5.0.0",
    "@itwin/core-frontend": "^5.0.0",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  }
}
