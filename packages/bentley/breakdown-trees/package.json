{
  "name": "@bentley/breakdown-trees-react",
  "version": "0.2.0",
  "description": "Package for breakdown trees",
  "keywords": [
    "Bentley",
    "iModel breakdown Tree",
    "iModel.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/imodeljs/viewer-components-react/tree/master/packages/bentley/breakdown-trees"
  },
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "http://www.bentley.com"
  },
  "main": "lib/breakdown-trees-react.js",
  "typings": "lib/breakdown-trees-react",
  "scripts": {
    "build": "npm run copy:assets && tsc --sourceMap 1>&2",
    "build-prod": "tsc 1>&2 && npm run copy:assets",
    "rebuild": "npm run clean && npm run build",
    "makegraph": "depcruise --exclude \"(node_modules|src/test)\" --output-type dot src > graphs/dependencygraph.svg",
    "copy:assets": "cpx \"./src/assets/**/*\" ./lib/assets && cpx \"./src/**/*.scss\" ./lib && cpx \"./public/**/*\" ./lib/public",
    "build:watch": "tsc -w",
    "clean": "rimraf lib",
    "cover": "npm test",
    "test": "mocha --opts mocha.opts \"./lib/test/**/*.test.js\"",
    "test:watch": "npm test -- --reporter min --watch-extensions ts,tsx --watch",
    "lint": "eslint -f visualstudio \"./src/**/*.{ts,tsx}\" 1>&2",
    "lint:fix": "npm run lint -- --fix"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "i18next": "^10.2.2"
  },
  "devDependencies": {
    "@itwin/core-bentley": "rc",
    "@itwin/build-tools": "rc",
    "@itwin/core-geometry": "rc",
    "@itwin/core-common": "rc",
    "@itwin/core-frontend": "rc",
    "@itwin/core-i18n": "rc",
    "@itwin/core-markup": "rc",
    "@itwin/core-quantity": "rc",
    "@bentley/itwin-client": "^2.19.9",
    "@itwin/presentation-common": "rc",
    "@itwin/presentation-components": "rc",
    "@itwin/presentation-frontend": "rc",
    "@itwin/eslint-plugin": "rc",
    "@bentley/product-settings-client": "^2.19.9",
    "@itwin/appui-abstract": "rc",
    "@itwin/components-react": "rc",
    "@itwin/core-react": "rc",
    "@itwin/appui-react": "rc",
    "@itwin/appui-layout-react": "rc",
    "@itwin/webgl-compatibility": "rc",
    "@bentley/webpack-tools": "1.14.1",
    "@itwin/measure-tools-react": "^0.1.2",
    "@testing-library/react": "^11.2.6",
    "@types/i18next": "^8.4.2",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.9.8",
    "@types/mock-require": "^2.0.0",
    "mock-local-storage": "^1.1.8",
    "mock-require": "^3.0.3",
    "@types/faker": "^4.1.0",
    "@types/sinon": "^7.5.1",
    "@types/mocha": "^5.2.7",
    "@types/chai": "4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chai-jest-snapshot": "^1.3.4",
    "@types/chai-spies": "^1.0.0",
    "@types/enzyme": "3.10.5",
    "@types/react-redux": "^7.1.9",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "eslint": "^7.11.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.4",
    "mocha": "^6.2.2",
    "chai": "^4.2.0",
    "nyc": "^15.1.0",
    "chai-as-promised": "^7.1.1",
    "chai-jest-snapshot": "^2.0.0",
    "chai-spies": "1.0.0",
    "sinon": "^7.5.0",
    "sinon-chai": "^3.2.0",
    "faker": "^4.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "3.4.4",
    "typemoq": "^2.1.0",
    "jsdom": "^13.2.0",
    "jsdom-global": "3.0.2",
    "cpx": "^1.5.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.0",
    "xmlhttprequest": "^1.8.0"
  },
  "peerDependencies": {
    "@itwin/core-bentley": "rc",
    "@itwin/core-geometry": "rc",
    "@bentley/imodelhub-client": "^2.19.9",
    "@itwin/core-common": "rc",
    "@itwin/core-frontend": "rc",
    "@itwin/core-i18n": "rc",
    "@itwin/core-markup": "rc",
    "@itwin/core-quantity": "rc",
    "@bentley/itwin-client": "^2.19.9",
    "@itwin/presentation-common": "rc",
    "@itwin/presentation-components": "rc",
    "@itwin/presentation-frontend": "rc",
    "@bentley/product-settings-client": "^2.19.9",
    "@itwin/appui-abstract": "rc",
    "@itwin/components-react": "rc",
    "@itwin/core-react": "rc",
    "@itwin/appui-react": "rc",
    "@itwin/appui-layout-react": "rc",
    "@itwin/webgl-compatibility": "rc",
    "@bentley/webpack-tools": "1.14.1",
    "@itwin/measure-tools-react": "^0.1.2",
    "react-redux": "^7.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "nyc": {
    "extends": "./node_modules/@itwin/build-tools/.nycrc",
    "require": [
      "ignore-styles",
      "jsdom-global/register",
      "source-map-support/register",
      "ts-node/register"
    ],
    "check-coverage": true,
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100
  },
  "eslintConfig": {
    "plugins": [
      "@itwin"
    ],
    "extends": "plugin:@itwin/ui",
    "rules": {
			"@typescript-eslint/unbound-method": "off",
      "no-duplicate-imports": "off",
      "@typescript-eslint/consistent-type-imports": "error"
    }
  }
}
