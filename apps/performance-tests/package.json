{
  "name": "vcr-performance-tests",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "benchmark:tree-widget": "cross-env NODE_OPTIONS=\"--import ../../node-hooks/ignore-styles/register.cjs\" npm run test:tree-widget -- -O BENCHMARK_OUTPUT_PATH=./tree-widget-benchmark.json",
    "build": "npm run link:deps && tsc",
    "clean": "rimraf lib temp",
    "docs": "betools extract --fileExt=ts --extractFrom=./src --recursive --out=./build/docs/extract",
    "lint": "eslint \"./src/**/*.ts\"",
    "test": "cross-env NODE_OPTIONS=\"--import ../../node-hooks/ignore-styles/register.cjs\" mocha --enable-source-maps --config ./.mocharc.json ./lib/**/*.test.js",
    "test:tree-widget": "cross-env NODE_OPTIONS=\"--import ../../node-hooks/ignore-styles/register.cjs\" mocha --enable-source-maps --config ./.mocharc.json ./lib/tree-widget/*.test.js",
    "test:recreate": "cross-env RECREATE=true npm run test",
    "link:deps": "node ./scripts/linkWorkspaceDeps.cjs"
  },
  "devDependencies": {
    "@bentley/functional-schema": "^1.0.4",
    "@itwin/appui-react": "~5.4.0",
    "@itwin/build-tools": "^4.11.1",
    "@itwin/core-backend": "^4.11.1",
    "@itwin/core-bentley": "^4.11.1",
    "@itwin/core-common": "^4.11.1",
    "@itwin/core-frontend": "^4.11.1",
    "@itwin/core-geometry": "^4.11.1",
    "@itwin/core-quantity": "^4.11.1",
    "@itwin/ecschema-metadata": "^4.11.1",
    "@itwin/ecschema-rpcinterface-common": "^4.11.1",
    "@itwin/ecschema-rpcinterface-impl": "^4.11.1",
    "@itwin/eslint-plugin": "^5.1.0",
    "@itwin/presentation-common": "^4.11.1",
    "@itwin/presentation-core-interop": "^1.3.1",
    "@itwin/presentation-hierarchies": "^1.4.2",
    "@itwin/presentation-testing": "^5.4.2",
    "@itwin/tree-widget-react": "workspace:*",
    "@itwin/presentation-shared": "^1.2.1",
    "@itwin/unified-selection": "^1.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.3",
    "@types/jsdom": "^21.1.7",
    "@types/sinon-chai": "^4.0.0",
    "as-table": "^1.0.55",
    "chai": "^5.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "global-jsdom": "^26.0.0",
    "mocha": "^11.2.2",
    "jsdom": "^26.1.0",
    "sinon-chai": "^4.0.0",
    "test-utilities": "workspace:*",
    "raf": "^3.4.0",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "ignore-styles": "^5.0.1",
    "typescript": "~5.6.0"
  }
}
